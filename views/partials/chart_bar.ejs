      <%# Project Card Example %>
      <div class="card shadow mb-4">
        <div class="card-header py-3">
          <h6 class="m-0 font-weight-bold text-primary">Users' Cycling Time This Month (only accessible to org)</h6>
        </div>
        <div class="card-body">

          <% users.forEach(user => { %>
          <h4 class="small font-weight-bold"><%= user.name %><span class="float-right"><%= user.time %></span></h4>
          <div class="progress mb-4">
            <div class="progress-bar bg-danger" role="progressbar" style="width: <%= user.min %>%" aria-valuenow="<%= user.min %>" aria-valuemin="0" aria-valuemax="100"></div>
          </div>
          <% }) %>
        </div>
      </div>